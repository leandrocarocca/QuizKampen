<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.quizkampen.gameController">
   <children>
      <AnchorPane layoutX="63.0" layoutY="56.0" prefHeight="199.0" prefWidth="475.0">
         <children>
            <Label fx:id="QuestionLabel" alignment="CENTER" layoutX="-5.0" layoutY="91.0" prefHeight="17.0" prefWidth="488.0" text="Question" textAlignment="CENTER" />
         </children>
      </AnchorPane>
      <GridPane fx:id="gameGridPane" gridLinesVisible="true" layoutX="136.0" layoutY="233.0" prefHeight="125.0" prefWidth="328.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="answer1Button" mnemonicParsing="false" onAction="#checkAnswer" prefHeight="242.0" prefWidth="370.0" text="Answer1" />
            <Button fx:id="answer2Button" mnemonicParsing="false" onAction="#checkAnswer" prefHeight="195.0" prefWidth="360.0" text="Answer2" GridPane.columnIndex="1" />
            <Button fx:id="answer3Button" mnemonicParsing="false" onAction="#checkAnswer" prefHeight="147.0" prefWidth="316.0" text="Answer3" GridPane.rowIndex="1" />
            <Button fx:id="answer4Button" mnemonicParsing="false" onAction="#checkAnswer" prefHeight="187.0" prefWidth="389.0" text="Answer4" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
